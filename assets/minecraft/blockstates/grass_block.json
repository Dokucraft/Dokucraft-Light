{ "multipart": [
    { "when": { "snowy": "false" },
      "apply": [
        { "model": "block/grass_block" },
        { "model": "block/grass_block/1" },
        { "model": "block/grass_block/2" },
        { "model": "block/grass_block/3" },
        { "model": "block/grass_block/4" },
        { "model": "block/grass_block/5" },
        { "model": "block/grass_block/6" },
        { "model": "block/grass_block/7" },
        { "model": "block/grass_block/8" },
        { "model": "block/grass_block/9" }
      ]
    },
    { "when": { "snowy": "true" },
      "apply": [
        { "model": "block/grass_block_snow/0" },
        { "model": "block/grass_block_snow/1" },
        { "model": "block/grass_block_snow/2" },
        { "model": "block/grass_block_snow/3" },
        { "model": "block/grass_block_snow/4" },
        { "model": "block/grass_block_snow/5" },
        { "model": "block/grass_block_snow/6" },
        { "model": "block/grass_block_snow/7" },
        { "model": "block/grass_block_snow/8" },
        { "model": "block/grass_block_snow/9" }
      ]
    },
    { "when": { "snowy": "false" },
      "apply": [
        { "model": "block/empty", "weight": 1500 },
        { "model": "block/grass_block/flowers", "weight": 1 },
        { "model": "block/grass_block/flowers1", "weight": 1 },
        { "model": "block/grass_block/flowers2", "weight": 1 },
        { "model": "block/grass_block/flowers3", "weight": 1 },
        { "model": "block/grass_block/flowers4", "weight": 1 },
        { "model": "block/grass_block/flowers5", "weight": 1 }
      ]
    }
  ]
}
